--file examples\agentsForall.smv
MODULE main
	VAR
		foo_AGENT1: boolean; --controlled
		foo_AGENT2: boolean; --controlled
		foo_AGENT3: boolean; --controlled
	CONSTANTS AGENT1, AGENT2, AGENT3, SUBAGENT_UNDEF, AGENT1, AGENT2, AGENT3;
	ASSIGN
		init(foo_AGENT1) := TRUE;
		init(foo_AGENT2) := TRUE;
		init(foo_AGENT3) := TRUE;
		next(foo_AGENT1) := !(foo_AGENT1);
		next(foo_AGENT2) := !(foo_AGENT2);
		next(foo_AGENT3) := !(foo_AGENT3);
--CTL properties
CTLSPEC NAME ctl0 := AG(((foo_AGENT1 = foo_AGENT2) & (foo_AGENT2 = foo_AGENT3)));
CTLSPEC NAME ctl1 := AG((foo_AGENT1 <-> AX(!(foo_AGENT1))));
CTLSPEC NAME ctl2 := AG((!(foo_AGENT1) <-> AX(foo_AGENT1)));
CTLSPEC NAME ctl3 := AG((foo_AGENT2 <-> AX(!(foo_AGENT2))));
CTLSPEC NAME ctl4 := AG((!(foo_AGENT2) <-> AX(foo_AGENT2)));
CTLSPEC NAME ctl5 := AG((foo_AGENT3 <-> AX(!(foo_AGENT3))));
CTLSPEC NAME ctl6 := AG((!(foo_AGENT3) <-> AX(foo_AGENT3)));
