--file examples\conditionalTerm.smv
MODULE main
	VAR
		foo_AA: {AA, BB, CC, DD, ENUMDOM_UNDEF}; --controlled
		foo_BB: {AA, BB, CC, DD, ENUMDOM_UNDEF}; --controlled
		foo_CC: {AA, BB, CC, DD, ENUMDOM_UNDEF}; --controlled
		foo_DD: {AA, BB, CC, DD, ENUMDOM_UNDEF}; --controlled
	CONSTANTS ENUMDOM_UNDEF, AA, BB, CC, DD;
	ASSIGN
		init(foo_AA) := BB;
		init(foo_BB) := ENUMDOM_UNDEF;
		init(foo_CC) := ENUMDOM_UNDEF;
		init(foo_DD) := ENUMDOM_UNDEF;
		next(foo_AA) := BB;
		next(foo_BB) := BB;
		next(foo_CC) := BB;
		next(foo_DD) := BB;
--CTL properties
CTLSPEC NAME ctl0 := AG((foo_AA = BB));
CTLSPEC NAME ctl1 := (((foo_BB = ENUMDOM_UNDEF) & (foo_CC = ENUMDOM_UNDEF)) & (foo_DD = ENUMDOM_UNDEF));
CTLSPEC NAME ctl2 := AX(AG(((foo_DD = BB) & ((foo_BB = BB) & (foo_CC = BB)))));
