--file examples\agentsAsyncr.smv
-- options: flatten? false
MODULE main
	VAR
		foo1: {-2147483647, 1, 2, 3, 4}; --controlled
		foo2: {-2147483647, 1, 2, 3, 4}; --controlled
	CONSTANTS AGENT2, SUBAGENT2_UNDEF, AGENT2, AGENT1, SUBAGENT1_UNDEF, AGENT1;
	ASSIGN
		init(foo1) := 1;
		init(foo2) := 1;
		next(foo1) :=
			case
				(foo1 < 4) & (foo1 + 1) in {-2147483647, 1, 2, 3, 4}: (foo1 + 1);
				(!(foo1 < 4)) & 1 in {-2147483647, 1, 2, 3, 4}: 1;
				TRUE: foo1;
			esac;
		next(foo2) :=
			case
				(foo2 < 4) & (foo2 + 1) in {-2147483647, 1, 2, 3, 4}: (foo2 + 1);
				(!(foo2 < 4)) & 1 in {-2147483647, 1, 2, 3, 4}: 1;
				TRUE: foo2;
			esac;
--CTL properties
CTLSPEC NAME ctl0 := AG((foo1 = foo2));
--AsmetaL invariants
CTLSPEC	AG((foo1 = foo2));
