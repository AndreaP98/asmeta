MODULE main
	VAR
		foo: {1, 2, 3, 4, -2147483647}; --controlled
		mon: boolean; --monitored
	ASSIGN
		init(foo) := -2147483647;
		next(foo) :=
			case
				(TRUE & 1 < 2 & !(mon)) & 3 in {1, 2, 3, 4, -2147483647}: 3;
				(TRUE & 1 < 2 & mon) & 2 in {1, 2, 3, 4, -2147483647}: 2;
				(TRUE & !(1 < 2)) & 1 in {1, 2, 3, 4, -2147483647}: 1;
				TRUE: foo;
			esac;
