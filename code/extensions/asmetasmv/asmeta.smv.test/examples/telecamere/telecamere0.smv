--file examples\telecamere\telecamere0.smv
MODULE main
	VAR
		teleChoice: 1..3; --monitored
		telecamere_1: boolean; --controlled
		telecamere_2: boolean; --controlled
		telecamere_3: boolean; --controlled
	ASSIGN
		init(telecamere_1) := TRUE;
		init(telecamere_2) := TRUE;
		init(telecamere_3) := TRUE;
		next(telecamere_1) :=
			case
				((((teleChoice = 1) & (((teleChoice > 1) & (teleChoice = 2)) & telecamere_1)) | ((((teleChoice = 1) & (teleChoice > 1)) & telecamere_0) & (teleChoice = 1))) | ((teleChoice = 1) & (((teleChoice = 3) & (teleChoice > 1)) & telecamere_2))): TRUE;
				TRUE: telecamere_1;
			esac;
		next(telecamere_2) :=
			case
				((((teleChoice = 2) & (((teleChoice > 1) & (teleChoice = 2)) & telecamere_1)) | ((((teleChoice = 1) & (teleChoice > 1)) & telecamere_0) & (teleChoice = 2))) | ((teleChoice = 2) & (((teleChoice = 3) & (teleChoice > 1)) & telecamere_2))): TRUE;
				TRUE: telecamere_2;
			esac;
		next(telecamere_3) :=
			case
				(((((teleChoice = 1) & (teleChoice > 1)) & telecamere_0) & (teleChoice = 3)) | (((teleChoice = 3) & (((teleChoice > 1) & (teleChoice = 2)) & telecamere_1)) | ((teleChoice = 3) & (((teleChoice = 3) & (teleChoice > 1)) & telecamere_2)))): TRUE;
				TRUE: telecamere_3;
			esac;
