--file examples\choose2.smv
MODULE main
	VAR
		var_b: {-2147483647, 1, 2, 3, 4}; --controlled
		--declaration of choose variables
		var_$x_0: 1..4;
		var_$y_0: 1..4;
	ASSIGN
		init(var_b) := 2;
		next(var_b) :=
			case
				2 in {-2147483647, 1, 2, 3, 4}: 2;
				TRUE: var_b;
			esac;
--CTL properties
CTLSPEC NAME ctl0 := AG((var_b = 2));
