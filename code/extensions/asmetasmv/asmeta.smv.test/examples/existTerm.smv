--file examples\existTerm.smv
-- options: flatten? false
MODULE main
	VAR
		foo1: boolean; --controlled
		foo2: boolean; --controlled
		foo3: boolean; --controlled
		foo_1: {-2147483647, 1, 2, 3}; --controlled
		foo_2: {-2147483647, 1, 2, 3}; --controlled
		foo_3: {-2147483647, 1, 2, 3}; --controlled
		var_a: {-2147483647, 1, 2, 3}; --controlled
		--declaration of choose variables
		var_$j_0: 1..3;
		var_$j_1: 1..3;
		var_$j_2: 1..3;
	ASSIGN
		init(foo1) := TRUE;
		init(foo2) := FALSE;
		init(foo3) := FALSE;
		init(foo_1) := 1;
		init(foo_2) := 1;
		init(foo_3) := 1;
		init(var_a) := 3;
		next(foo1) := ((foo_1 = 1) | (foo_2 = 1) | (foo_3 = 1));
		next(foo2) := ((foo_1 = 2) | (foo_2 = 2) | (foo_3 = 2));
		next(foo3) := ((foo_1 = 3) | (foo_3 = 3) | (foo_2 = 3));
		next(foo_1) :=
			case
				(var_$j_0 = 3) & 3 in {-2147483647, 1, 2, 3}: 3;
				(var_$j_0 = 2) & 2 in {-2147483647, 1, 2, 3}: 2;
				(var_$j_0 = 1) & 1 in {-2147483647, 1, 2, 3}: 1;
				TRUE: foo_1;
			esac;
		next(foo_2) :=
			case
				(var_$j_1 = 3) & 3 in {-2147483647, 1, 2, 3}: 3;
				(var_$j_1 = 1) & 1 in {-2147483647, 1, 2, 3}: 1;
				(var_$j_1 = 2) & 2 in {-2147483647, 1, 2, 3}: 2;
				TRUE: foo_2;
			esac;
		next(foo_3) :=
			case
				(var_$j_2 = 1) & 1 in {-2147483647, 1, 2, 3}: 1;
				(var_$j_2 = 3) & 3 in {-2147483647, 1, 2, 3}: 3;
				(var_$j_2 = 2) & 2 in {-2147483647, 1, 2, 3}: 2;
				TRUE: foo_3;
			esac;
		next(var_a) :=
			case
				3 in {-2147483647, 1, 2, 3}: 3;
				TRUE: var_a;
			esac;
	INVAR	((var_$j_0 = 3) | (var_$j_0 = 2) | (var_$j_0 = 1));
	INVAR	((var_$j_1 = 3) | (var_$j_1 = 1) | (var_$j_1 = 2));
	INVAR	((var_$j_2 = 1) | (var_$j_2 = 3) | (var_$j_2 = 2));
--CTL properties
CTLSPEC NAME ctl0 := AG((var_a = 3));
CTLSPEC NAME ctl1 := AG((((foo_1 = 1) | (foo_2 = 1) | (foo_3 = 1)) <-> AX(foo1)));
CTLSPEC NAME ctl2 := AG((((foo_1 = 2) | (foo_2 = 2) | (foo_3 = 2)) <-> AX(foo2)));
CTLSPEC NAME ctl3 := AG((((foo_1 = 3) | (foo_3 = 3) | (foo_2 = 3)) <-> AX(foo3)));
