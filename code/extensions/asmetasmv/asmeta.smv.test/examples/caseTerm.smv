--file examples\caseTerm.smv
MODULE main
	VAR
		foo2_AA: {AA, BB, CC, DD, ENUMDOM_UNDEF}; --controlled
		foo2_BB: {AA, BB, CC, DD, ENUMDOM_UNDEF}; --controlled
		foo2_CC: {AA, BB, CC, DD, ENUMDOM_UNDEF}; --controlled
		foo2_DD: {AA, BB, CC, DD, ENUMDOM_UNDEF}; --controlled
		foo3_AA: {AA, BB, CC, DD, ENUMDOM_UNDEF}; --controlled
		foo3_BB: {AA, BB, CC, DD, ENUMDOM_UNDEF}; --controlled
		foo3_CC: {AA, BB, CC, DD, ENUMDOM_UNDEF}; --controlled
		foo3_DD: {AA, BB, CC, DD, ENUMDOM_UNDEF}; --controlled
		foo_AA: {AA, BB, CC, DD, ENUMDOM_UNDEF}; --controlled
		foo_BB: {AA, BB, CC, DD, ENUMDOM_UNDEF}; --controlled
		foo_CC: {AA, BB, CC, DD, ENUMDOM_UNDEF}; --controlled
		foo_DD: {AA, BB, CC, DD, ENUMDOM_UNDEF}; --controlled
	CONSTANTS ENUMDOM_UNDEF, AA, BB, CC, DD;
	ASSIGN
		init(foo2_AA) := AA;
		init(foo2_BB) := BB;
		init(foo2_CC) := CC;
		init(foo2_DD) := ENUMDOM_UNDEF;
		init(foo3_AA) := AA;
		init(foo3_BB) := BB;
		init(foo3_CC) := DD;
		init(foo3_DD) := DD;
		init(foo_AA) := BB;
		init(foo_BB) := CC;
		init(foo_CC) := ENUMDOM_UNDEF;
		init(foo_DD) := ENUMDOM_UNDEF;
		next(foo2_AA) := AA;
		next(foo2_BB) := BB;
		next(foo2_CC) := CC;
		next(foo2_DD) := foo2_DD;
		next(foo3_AA) := foo3_AA;
		next(foo3_BB) := foo3_BB;
		next(foo3_CC) := foo3_CC;
		next(foo3_DD) := foo3_DD;
		next(foo_AA) := BB;
		next(foo_BB) := BB;
		next(foo_CC) := BB;
		next(foo_DD) := BB;
--CTL properties
CTLSPEC NAME ctl0 := (((foo_CC = ENUMDOM_UNDEF) & ((foo_BB = CC) & (foo_AA = BB))) & (foo_DD = ENUMDOM_UNDEF));
CTLSPEC NAME ctl1 := AG((foo_AA = BB));
CTLSPEC NAME ctl2 := AX(AG((foo_BB = BB)));
CTLSPEC NAME ctl3 := AX(AG((foo_CC = BB)));
CTLSPEC NAME ctl4 := AX(AG((foo_DD = BB)));
CTLSPEC NAME ctl5 := AG(((foo2_AA = AA) & (foo2_CC = CC) & (foo2_BB = BB)));
CTLSPEC NAME ctl6 := AG((foo2_DD = ENUMDOM_UNDEF));
CTLSPEC NAME ctl7 := AG((((foo3_CC = DD) & ((foo3_BB = BB) & (foo3_AA = AA))) & (foo3_DD = DD)));
