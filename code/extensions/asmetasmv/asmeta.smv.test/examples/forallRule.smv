--file examples\forallRule.smv
-- options: flatten? false
MODULE main
	VAR
		foo_1: {-2147483647, 1, 2, 3, 4}; --controlled
		foo_2: {-2147483647, 1, 2, 3, 4}; --controlled
		foo_3: {-2147483647, 1, 2, 3, 4}; --controlled
		foo_4: {-2147483647, 1, 2, 3, 4}; --controlled
	ASSIGN
		init(foo_1) := 4;
		init(foo_2) := 4;
		init(foo_3) := 4;
		init(foo_4) := 4;
		next(foo_1) :=
			case
				((foo_4 - 1) = 1) & 0 in {-2147483647, 1, 2, 3, 4}: 0;
				3 in {-2147483647, 1, 2, 3, 4}: 3;
				TRUE: foo_1;
			esac;
		next(foo_2) :=
			case
				((foo_4 - 1) = 2) & 1 in {-2147483647, 1, 2, 3, 4}: 1;
				2 in {-2147483647, 1, 2, 3, 4}: 2;
				TRUE: foo_2;
			esac;
		next(foo_3) :=
			case
				((foo_4 - 1) = 3) & 2 in {-2147483647, 1, 2, 3, 4}: 2;
				TRUE: foo_3;
			esac;
		next(foo_4) :=
			case
				((foo_4 - 1) = 4) & 3 in {-2147483647, 1, 2, 3, 4}: 3;
				TRUE: foo_4;
			esac;
--CTL properties
CTLSPEC NAME ctl0 := AX(AG(((foo_1 = 3) & (foo_2 = 2))));
CTLSPEC NAME ctl1 := AG((foo_4 = 4));
CTLSPEC NAME ctl2 := AX(AG((foo_3 = 2)));
