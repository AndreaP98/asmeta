--file examples\derivedNotExhaustive.smv
MODULE main
	VAR
		foo: {-2147483647, 1, 2, 3, 4}; --controlled
		mon1: boolean; --monitored
		mon2: boolean; --monitored
	DEFINE
		der :=
			case
				(mon1 & !(mon2)): FALSE;
				(mon2 & mon1): TRUE;
				TRUE: FALSE;
			esac;
	ASSIGN
		init(foo) := -2147483647;
		next(foo) :=
			case
				(der) & 1 in {-2147483647, 1, 2, 3, 4}: 1;
				TRUE: foo;
			esac;
