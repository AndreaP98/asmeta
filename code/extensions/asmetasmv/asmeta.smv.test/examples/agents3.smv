--file examples\agents3.smv
MODULE main
	VAR
		foo12_AGENT1: boolean; --controlled
		foo12_AGENT2: boolean; --controlled
	CONSTANTS AGENT1, AGENT2, SUBAGENT_UNDEF, AGENT1, AGENT2;
	ASSIGN
		init(foo12_AGENT1) := TRUE;
		init(foo12_AGENT2) := FALSE;
		next(foo12_AGENT1) := !(foo12_AGENT1);
		next(foo12_AGENT2) := !(foo12_AGENT2);
--CTL properties
CTLSPEC NAME ctl0 := AG((foo12_AGENT1 != foo12_AGENT2));
