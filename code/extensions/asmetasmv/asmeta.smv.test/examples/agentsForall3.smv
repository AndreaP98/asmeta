--file examples\agentsForall3.smv
-- options: flatten? false
MODULE main
	VAR
		chosenId: 1..3; --monitored
		foo_AGENT1: boolean; --controlled
		foo_AGENT2: boolean; --controlled
		foo_AGENT3: boolean; --controlled
	DEFINE
	CONSTANTS AGENT1, AGENT2, AGENT3, SUBAGENT_UNDEF, AGENT1, AGENT2, AGENT3;
	ASSIGN
		init(foo_AGENT1) := TRUE;
		init(foo_AGENT2) := TRUE;
		init(foo_AGENT3) := TRUE;
		next(foo_AGENT1) :=
			case
				(1 = chosenId): !(foo_AGENT1);
				TRUE: foo_AGENT1;
			esac;
		next(foo_AGENT2) :=
			case
				(2 = chosenId): !(foo_AGENT2);
				TRUE: foo_AGENT2;
			esac;
		next(foo_AGENT3) :=
			case
				(3 = chosenId): !(foo_AGENT3);
				TRUE: foo_AGENT3;
			esac;
--CTL properties
CTLSPEC NAME ctl0 := EF(((!(foo_AGENT2) & !(foo_AGENT1)) & !(foo_AGENT3)));
CTLSPEC NAME ctl1 := !(EG(((!(foo_AGENT2) & !(foo_AGENT1)) & !(foo_AGENT3))));
CTLSPEC NAME ctl2 := !(EG(((foo_AGENT1 = foo_AGENT2) & (foo_AGENT2 = foo_AGENT3))));
