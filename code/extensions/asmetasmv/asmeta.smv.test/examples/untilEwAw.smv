--file examples\untilEwAw.smv
MODULE main
	VAR
		foo: {-2147483647, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}; --controlled
		fooA: {-2147483647, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}; --controlled
		--declaration of choose variables
		var_$x_0: 0..10;
	ASSIGN
		init(foo) := 0;
		init(fooA) := 0;
		next(foo) :=
			case
				(foo < 10) & (foo + 1) in {-2147483647, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}: (foo + 1);
				TRUE: foo;
			esac;
		next(fooA) :=
			case
				((!(foo < 10) & !(fooA = 0) & (var_$x_0 = 1)) | (!(foo < 10) & (fooA = 0))) & 1 in {-2147483647, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}: 1;
				(!(foo < 10) & !(fooA = 0) & (var_$x_0 = 10)) & 10 in {-2147483647, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}: 10;
				(!(foo < 10) & !(fooA = 0) & (var_$x_0 = 2)) & 2 in {-2147483647, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}: 2;
				(!(foo < 10) & !(fooA = 0) & (var_$x_0 = 4)) & 4 in {-2147483647, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}: 4;
				(!(foo < 10) & !(fooA = 0) & (var_$x_0 = 3)) & 3 in {-2147483647, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}: 3;
				(!(foo < 10) & !(fooA = 0) & (var_$x_0 = 6)) & 6 in {-2147483647, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}: 6;
				(!(foo < 10) & !(fooA = 0) & (var_$x_0 = 5)) & 5 in {-2147483647, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}: 5;
				(!(foo < 10) & !(fooA = 0) & (var_$x_0 = 8)) & 8 in {-2147483647, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}: 8;
				(!(foo < 10) & !(fooA = 0) & (var_$x_0 = 7)) & 7 in {-2147483647, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}: 7;
				(!(foo < 10) & !(fooA = 0) & (var_$x_0 = 9)) & 9 in {-2147483647, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}: 9;
				TRUE: fooA;
			esac;
	INVAR	((var_$x_0 = 10) | (var_$x_0 = 6) | (var_$x_0 = 7) | (var_$x_0 = 4) | (var_$x_0 = 5) | (var_$x_0 = 2) | (var_$x_0 = 3) | (var_$x_0 = 1) | (var_$x_0 = 8) | (var_$x_0 = 9));
--CTL properties
CTLSPEC NAME ctl0 := A[(foo < 10) U AX((fooA = 1))];
CTLSPEC NAME ctl1 := E[(foo <= 10) U AG((fooA = 1))] | EG((foo <= 10));
