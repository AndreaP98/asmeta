--file examples\integerConcrDom.smv
MODULE main
	VAR
		foo: boolean; --controlled
		fooA: {-1, -10, -2, -2147483647, -3, -4, -5, -6, -7, -8, -9, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}; --controlled
		fooB: {-1, -10, -2, -2147483647, -3, -4, -5, -6, -7, -8, -9, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}; --controlled
	DEFINE
	ASSIGN
		init(foo) := TRUE;
		init(fooA) := -2147483647;
		init(fooB) := -2147483647;
		next(foo) := TRUE;
		next(fooA) :=
			case
				6 in {-1, -10, -2, -2147483647, -3, -4, -5, -6, -7, -8, -9, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}: 6;
				TRUE: fooA;
			esac;
		next(fooB) :=
			case
				-5 in {-1, -10, -2, -2147483647, -3, -4, -5, -6, -7, -8, -9, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}: -5;
				TRUE: fooB;
			esac;
--CTL properties
CTLSPEC NAME ctl0 := AG(foo);
