--file examples\forall3.smv
MODULE main
	VAR
		foo_1: {-2147483647, 1, 2, 3}; --controlled
		foo_2: {-2147483647, 1, 2, 3}; --controlled
		foo_3: {-2147483647, 1, 2, 3}; --controlled
	ASSIGN
		init(foo_1) := 1;
		init(foo_2) := 2;
		init(foo_3) := 3;
		next(foo_1) :=
			case
				1 in {-2147483647, 1, 2, 3}: 1;
				TRUE: foo_1;
			esac;
		next(foo_2) :=
			case
				2 in {-2147483647, 1, 2, 3}: 2;
				TRUE: foo_2;
			esac;
		next(foo_3) :=
			case
				3 in {-2147483647, 1, 2, 3}: 3;
				TRUE: foo_3;
			esac;
--CTL properties
CTLSPEC NAME ctl0 := AG(((foo_3 = 3) & ((foo_1 = 1) & (foo_2 = 2))));
