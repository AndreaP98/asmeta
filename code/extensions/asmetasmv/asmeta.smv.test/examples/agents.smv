--file examples\agents.smv
-- options: flatten? false
MODULE main
	VAR
		example: boolean; --controlled
		fooAB_AGENTA: boolean; --controlled
		fooAB_AGENTB: boolean; --controlled
		fooC_AGENTC: {-2147483647, 1, 2, 3, 4}; --controlled
		fooChoose_1: {-2147483647, 1, 2, 3, 4}; --controlled
		fooChoose_2: {-2147483647, 1, 2, 3, 4}; --controlled
		fooChoose_3: {-2147483647, 1, 2, 3, 4}; --controlled
		fooChoose_4: {-2147483647, 1, 2, 3, 4}; --controlled
		fooD_AGENTD: {AA, BB, ENUMDOM_UNDEF}; --controlled
		fooEnum_AA: {AA, BB, ENUMDOM_UNDEF}; --controlled
		fooEnum_BB: {AA, BB, ENUMDOM_UNDEF}; --controlled
		fooProd_1_1: {-2147483647, 1, 2, 3, 4}; --controlled
		fooProd_1_2: {-2147483647, 1, 2, 3, 4}; --controlled
		fooProd_1_3: {-2147483647, 1, 2, 3, 4}; --controlled
		fooProd_1_4: {-2147483647, 1, 2, 3, 4}; --controlled
		fooProd_2_1: {-2147483647, 1, 2, 3, 4}; --controlled
		fooProd_2_2: {-2147483647, 1, 2, 3, 4}; --controlled
		fooProd_2_3: {-2147483647, 1, 2, 3, 4}; --controlled
		fooProd_2_4: {-2147483647, 1, 2, 3, 4}; --controlled
		fooProd_3_1: {-2147483647, 1, 2, 3, 4}; --controlled
		fooProd_3_2: {-2147483647, 1, 2, 3, 4}; --controlled
		fooProd_3_3: {-2147483647, 1, 2, 3, 4}; --controlled
		fooProd_3_4: {-2147483647, 1, 2, 3, 4}; --controlled
		fooProd_4_1: {-2147483647, 1, 2, 3, 4}; --controlled
		fooProd_4_2: {-2147483647, 1, 2, 3, 4}; --controlled
		fooProd_4_3: {-2147483647, 1, 2, 3, 4}; --controlled
		fooProd_4_4: {-2147483647, 1, 2, 3, 4}; --controlled
		foo_1: {-2147483647, 1, 2, 3, 4}; --controlled
		foo_2: {-2147483647, 1, 2, 3, 4}; --controlled
		foo_3: {-2147483647, 1, 2, 3, 4}; --controlled
		foo_4: {-2147483647, 1, 2, 3, 4}; --controlled
		--declaration of choose variables
		var_$w_1: 1..4;
		var_$w_2: 1..4;
		var_$w_3: 1..4;
		var_$w_4: 1..4;
		var_$x_0: 1..4;
		var_$y_0: 1..4;
	CONSTANTS AGENTD, AGENTA, AGENTB, SUBAGENT1_UNDEF, AGENTA, AGENTB, AGENTC, SUBAGENT2_UNDEF, AGENTC, ENUMDOM_UNDEF, AA, BB;
	ASSIGN
		init(fooAB_AGENTA) := TRUE;
		init(fooAB_AGENTB) := FALSE;
		init(fooC_AGENTC) := 1;
		init(fooChoose_1) := 1;
		init(fooChoose_2) := 2;
		init(fooChoose_3) := 3;
		init(fooChoose_4) := 4;
		init(fooD_AGENTD) := AA;
		init(fooEnum_AA) := AA;
		init(fooEnum_BB) := BB;
		init(fooProd_1_1) := -2147483647;
		init(fooProd_1_2) := -2147483647;
		init(fooProd_1_3) := -2147483647;
		init(fooProd_1_4) := -2147483647;
		init(fooProd_2_1) := -2147483647;
		init(fooProd_2_2) := -2147483647;
		init(fooProd_2_3) := -2147483647;
		init(fooProd_2_4) := -2147483647;
		init(fooProd_3_1) := -2147483647;
		init(fooProd_3_2) := -2147483647;
		init(fooProd_3_3) := -2147483647;
		init(fooProd_3_4) := -2147483647;
		init(fooProd_4_1) := -2147483647;
		init(fooProd_4_2) := -2147483647;
		init(fooProd_4_3) := -2147483647;
		init(fooProd_4_4) := -2147483647;
		init(foo_1) := 4;
		init(foo_2) := 4;
		init(foo_3) := 4;
		init(foo_4) := 4;
		next(example) := TRUE;
		next(fooAB_AGENTA) := !(fooAB_AGENTA);
		next(fooAB_AGENTB) := !(fooAB_AGENTB);
		next(fooC_AGENTC) :=
			case
				(!(fooC_AGENTC < 4)) & 1 in {-2147483647, 1, 2, 3, 4}: 1;
				(fooC_AGENTC < 4) & (fooC_AGENTC + 1) in {-2147483647, 1, 2, 3, 4}: (fooC_AGENTC + 1);
				TRUE: fooC_AGENTC;
			esac;
		next(fooChoose_1) :=
			case
				(var_$w_1 = 1) & 1 in {-2147483647, 1, 2, 3, 4}: 1;
				TRUE: fooChoose_1;
			esac;
		next(fooChoose_2) :=
			case
				(var_$w_2 = 1) & 1 in {-2147483647, 1, 2, 3, 4}: 1;
				(var_$w_2 = 2) & 2 in {-2147483647, 1, 2, 3, 4}: 2;
				TRUE: fooChoose_2;
			esac;
		next(fooChoose_3) :=
			case
				(var_$w_3 = 1) & 1 in {-2147483647, 1, 2, 3, 4}: 1;
				(var_$w_3 = 3) & 3 in {-2147483647, 1, 2, 3, 4}: 3;
				(var_$w_3 = 2) & 2 in {-2147483647, 1, 2, 3, 4}: 2;
				TRUE: fooChoose_3;
			esac;
		next(fooChoose_4) :=
			case
				(var_$w_4 = 1) & 1 in {-2147483647, 1, 2, 3, 4}: 1;
				(var_$w_4 = 2) & 2 in {-2147483647, 1, 2, 3, 4}: 2;
				(var_$w_4 = 3) & 3 in {-2147483647, 1, 2, 3, 4}: 3;
				(var_$w_4 = 4) & 4 in {-2147483647, 1, 2, 3, 4}: 4;
				TRUE: fooChoose_4;
			esac;
		next(fooD_AGENTD) := BB;
		next(fooEnum_AA) := AA;
		next(fooEnum_BB) :=
			case
				(!(fooEnum_BB = BB)): AA;
				(fooEnum_BB = BB): BB;
				TRUE: fooEnum_BB;
			esac;
		next(fooProd_1_1) :=
			case
				1 in {-2147483647, 1, 2, 3, 4}: 1;
				TRUE: fooProd_1_1;
			esac;
		next(fooProd_1_2) :=
			case
				2 in {-2147483647, 1, 2, 3, 4}: 2;
				TRUE: fooProd_1_2;
			esac;
		next(fooProd_1_3) :=
			case
				3 in {-2147483647, 1, 2, 3, 4}: 3;
				TRUE: fooProd_1_3;
			esac;
		next(fooProd_1_4) :=
			case
				4 in {-2147483647, 1, 2, 3, 4}: 4;
				TRUE: fooProd_1_4;
			esac;
		next(fooProd_2_1) :=
			case
				1 in {-2147483647, 1, 2, 3, 4}: 1;
				TRUE: fooProd_2_1;
			esac;
		next(fooProd_2_2) :=
			case
				2 in {-2147483647, 1, 2, 3, 4}: 2;
				TRUE: fooProd_2_2;
			esac;
		next(fooProd_2_3) :=
			case
				3 in {-2147483647, 1, 2, 3, 4}: 3;
				TRUE: fooProd_2_3;
			esac;
		next(fooProd_2_4) :=
			case
				4 in {-2147483647, 1, 2, 3, 4}: 4;
				TRUE: fooProd_2_4;
			esac;
		next(fooProd_3_1) :=
			case
				1 in {-2147483647, 1, 2, 3, 4}: 1;
				TRUE: fooProd_3_1;
			esac;
		next(fooProd_3_2) :=
			case
				2 in {-2147483647, 1, 2, 3, 4}: 2;
				TRUE: fooProd_3_2;
			esac;
		next(fooProd_3_3) :=
			case
				3 in {-2147483647, 1, 2, 3, 4}: 3;
				TRUE: fooProd_3_3;
			esac;
		next(fooProd_3_4) :=
			case
				4 in {-2147483647, 1, 2, 3, 4}: 4;
				TRUE: fooProd_3_4;
			esac;
		next(fooProd_4_1) :=
			case
				1 in {-2147483647, 1, 2, 3, 4}: 1;
				TRUE: fooProd_4_1;
			esac;
		next(fooProd_4_2) :=
			case
				2 in {-2147483647, 1, 2, 3, 4}: 2;
				TRUE: fooProd_4_2;
			esac;
		next(fooProd_4_3) :=
			case
				3 in {-2147483647, 1, 2, 3, 4}: 3;
				TRUE: fooProd_4_3;
			esac;
		next(fooProd_4_4) :=
			case
				4 in {-2147483647, 1, 2, 3, 4}: 4;
				TRUE: fooProd_4_4;
			esac;
		next(foo_1) :=
			case
				((var_$y_0 = 3) & (var_$x_0 = 1)) & 3 in {-2147483647, 1, 2, 3, 4}: 3;
				((var_$y_0 = 4) & (var_$x_0 = 1)) & 4 in {-2147483647, 1, 2, 3, 4}: 4;
				((var_$y_0 = 2) & (var_$x_0 = 1)) & 2 in {-2147483647, 1, 2, 3, 4}: 2;
				TRUE: foo_1;
			esac;
		next(foo_2) :=
			case
				((var_$y_0 = 3) & (var_$x_0 = 2)) & 3 in {-2147483647, 1, 2, 3, 4}: 3;
				((var_$y_0 = 4) & (var_$x_0 = 2)) & 4 in {-2147483647, 1, 2, 3, 4}: 4;
				TRUE: foo_2;
			esac;
		next(foo_3) :=
			case
				((var_$y_0 = 4) & (var_$x_0 = 3)) & 4 in {-2147483647, 1, 2, 3, 4}: 4;
				TRUE: foo_3;
			esac;
		next(foo_4) := foo_4;
	INVAR	(((var_$y_0 = 3) & (var_$x_0 = 2)) | ((var_$y_0 = 3) & (var_$x_0 = 1)) | ((var_$y_0 = 4) & (var_$x_0 = 3)) | ((var_$y_0 = 4) & (var_$x_0 = 2)) | ((var_$y_0 = 4) & (var_$x_0 = 1)) | ((var_$y_0 = 2) & (var_$x_0 = 1)));
	INVAR	(var_$w_1 = 1);
	INVAR	((var_$w_2 = 1) | (var_$w_2 = 2));
	INVAR	((var_$w_3 = 1) | (var_$w_3 = 3) | (var_$w_3 = 2));
	INVAR	((var_$w_4 = 1) | (var_$w_4 = 2) | (var_$w_4 = 3) | (var_$w_4 = 4));
--CTL properties
CTLSPEC NAME ctl0 := AG((fooAB_AGENTA != fooAB_AGENTB));
CTLSPEC NAME ctl1 := AG((fooAB_AGENTA <-> ((fooC_AGENTC mod 2) = 1)));
CTLSPEC NAME ctl2 := AG((foo_2 > 2));
CTLSPEC NAME ctl3 := AG((foo_3 > 3));
CTLSPEC NAME ctl4 := (fooChoose_1 <= 1);
CTLSPEC NAME ctl5 := AG((fooChoose_2 <= 2));
CTLSPEC NAME ctl6 := AG((fooChoose_3 <= 3));
