--file examples\chooseChoose.smv
MODULE main
	VAR
		fooA: boolean; --controlled
		fooB: boolean; --controlled
		fooC: boolean; --controlled
		--declaration of choose variables
		var_$c_4: 1..4;
		var_$i_3: 1..4;
		var_$k_2: 1..4;
		var_$x_0: 1..4;
		var_$y_1: 1..4;
		var_$z_5: 1..4;
	ASSIGN
		init(fooA) := FALSE;
		init(fooB) := FALSE;
		init(fooC) := FALSE;
		next(fooA) :=
			case
				((var_$y_1 = 1) & (var_$x_0 = 1)): TRUE;
				TRUE: fooA;
			esac;
		next(fooB) :=
			case
				(var_$k_2 = 1): FALSE;
				TRUE: fooB;
			esac;
		next(fooC) := fooC;
	INVAR	(var_$x_0 = 1);
	INVAR	((((var_$x_0 = 4) & (var_$y_1 = 3)) | ((var_$x_0 = 4) & (var_$y_1 = 4)) | ((var_$x_0 = 4) & (var_$y_1 = 1)) | ((var_$x_0 = 4) & (var_$y_1 = 2))) | (((((var_$y_1 = 1) & (var_$x_0 = 2)) | ((var_$x_0 = 2) & (var_$y_1 = 2))) | ((var_$y_1 = 1) & (var_$x_0 = 1))) | (((var_$y_1 = 2) & (var_$x_0 = 3)) | ((var_$y_1 = 1) & (var_$x_0 = 3)) | ((var_$x_0 = 3) & (var_$y_1 = 3)))));
	INVAR	(var_$k_2 = 1);
	INVAR	(var_$c_4 = 1);
--CTL properties
CTLSPEC NAME ctl0 := (AX(AG(fooA)) & !(fooA));
CTLSPEC NAME ctl1 := AG(!(fooB));
CTLSPEC NAME ctl2 := AG(!(fooC));
