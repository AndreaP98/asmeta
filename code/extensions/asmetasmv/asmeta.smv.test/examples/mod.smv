		fooZ: {-2147483647, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}; --controlled
	ASSIGN
		init(fooA_I) := 2;
		init(fooA_N) := 2;
		init(fooB_I) := 0;
		init(fooB_N) := 0;
		init(fooC_I) := 0;
		init(fooC_N) := 0;
		init(fooD_I) := 0;
		init(fooD_N) := 0;
		init(fooE_I) := 3;
		init(fooE_N) := 3;
