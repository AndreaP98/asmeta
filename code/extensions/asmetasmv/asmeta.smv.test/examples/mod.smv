		next(fooG) :=
			case
				-1 in {-1, -10, -2, -2147483647, -3, -4, -5, -6, -7, -8, -9, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}: -1;
				TRUE: fooG;
			esac;
		next(fooH) :=
			case
				-1 in {-1, -10, -2, -2147483647, -3, -4, -5, -6, -7, -8, -9, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}: -1;
				TRUE: fooH;
			esac;
		next(fooZ) :=
			case
				(fooD_I mod 5) in {-2147483647, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}: (fooD_I mod 5);
				TRUE: fooZ;
			esac;
