		init(fooG) := -1;
		init(fooH) := -1;
		init(fooZ) := -2147483647;
		next(fooA_I) :=
			case
				2 in {-2147483647, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}: 2;
				TRUE: fooA_I;
			esac;
		next(fooA_N) :=
			case
				2 in {-2147483647, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}: 2;
				TRUE: fooA_N;
			esac;
		next(fooB_I) :=
			case
				0 in {-2147483647, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}: 0;
				TRUE: fooB_I;
			esac;
		next(fooB_N) :=
			case
				0 in {-2147483647, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}: 0;
				TRUE: fooB_N;
			esac;
		next(fooC_I) :=
			case
