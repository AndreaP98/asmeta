				TRUE: fooD_I;
			esac;
		next(fooD_N) :=
			case
				0 in {-2147483647, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}: 0;
				TRUE: fooD_N;
			esac;
		next(fooE_I) :=
			case
				3 in {-2147483647, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}: 3;
				TRUE: fooE_I;
			esac;
		next(fooE_N) :=
			case
				3 in {-2147483647, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}: 3;
				TRUE: fooE_N;
			esac;
		next(fooF) :=
			case
				1 in {-1, -10, -2, -2147483647, -3, -4, -5, -6, -7, -8, -9, 0, 1, 10, 2, 3, 4, 5, 6, 7, 8, 9}: 1;
				TRUE: fooF;
